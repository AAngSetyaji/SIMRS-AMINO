	<queryString>
		<![CDATA[select billing.`no`,billing.nm_perawatan,billing.pemisah,billing.tgl_byr,
CASE WHEN billing.biaya = 0 THEN '' ELSE biaya END AS biaya_display,
CASE WHEN billing.jumlah = 0 THEN '' ELSE jumlah END AS jumlah_display,
CASE WHEN billing.tambahan = 0 THEN '' ELSE tambahan END AS tambahan_display,
CASE WHEN billing.totalbiaya = 0 THEN '' ELSE CAST(totalbiaya AS CHAR) END AS totalbiaya_display,
piutang_pasien.totalpiutang,piutang_pasien.uangmuka,piutang_pasien.sisapiutang 
from billing 
INNER JOIN piutang_pasien ON piutang_pasien.no_rawat=billing.no_rawat]]>
	</queryString>
