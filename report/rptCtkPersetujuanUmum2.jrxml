				<text><![CDATA[A. ]]></text>
			</staticText>
			<staticText>
				<reportElement x="30" y="35" width="10" height="10" uuid="66968171-4998-4295-8984-1c64c0b050a2"/>
				<textElement textAlignment="Left" verticalAlignment="Top">
